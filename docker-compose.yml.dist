drupalphp55devweb:
  build: .
  ports:
    - "82:80"
    - "445:443"
  links:
   - drupalphp55devdb:database
drupalphp55devdb:
  image: mysql:5.5
  volumes_from:
    - mysql-drupal-php55-dev
  environment:
    TERM: xterm
    MYSQL_ROOT_PASSWORD: password
    MYSQL_USER: user
    MYSQL_PASSWORD: password
    MYSQL_DATABASE: drupalphp55dev
